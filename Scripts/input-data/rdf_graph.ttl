:SmokedSalmonPasta rdf:type :Recipe ;
    rdfs:label "Smoked Salmon Pasta" ;
    rdfs:comment "Spinach, spring onion, lemon, curds & Parmesan" ;
    :categories "Pasta" ;
    :ingredients (:LasagneSheets :SpringOnion :Spinach :SmokedSalmon :Lemon :Parmesan :CottageCheese :OliveOil :BoilingWater) ;
    :steps (:Step1 :Step2 :Step3 :Step4 :Step5) .

:Step1 rdf:type :Step ;
    :order 1 ;
    :utensils (:Kettle :Knife) ;
    :inputs (:LasagneSheets) ;
    :outputs (:CutLasagneSheets) ;
    :Action "Cut" .

:Step2 rdf:type :Step ;
    :order 2 ;
    :utensils (:Knife) ;
    :inputs (:SpringOnion :Spinach :SmokedSalmonHalf :Lemon :Parmesan) ;
    :outputs (:ChoppedSpringOnion :ChoppedSpinach :ChoppedSalmon :LemonZest :GratedParmesan) ;
    :Action "Chop and Grate" .

:Step3 rdf:type :Step ;
    :order 3 ;
    :utensils (:FryingPan) ;
    :inputs (:OliveOil :ChoppedSpringOnion :ChoppedSpinach :ChoppedSalmon :LemonZest :CutLasagneSheets :BoilingWater) ;
    :outputs (:CookedPastaSauce) ;
    :Action "Cook" .

:Step4 rdf:type :Step ;
    :order 4 ;
    :inputs (:CookedPastaSauce :LemonJuice :CottageCheese :GratedParmesan) ;
    :outputs (:SeasonedPasta) ;
    :Action "Mix and Season" .

:Step5 rdf:type :Step ;
    :order 5 ;
    :inputs (:SeasonedPasta :RemainingSalmon :OliveOil) ;
    :outputs (:FinalDish) ;
    :Action "Finish and Serve" .

